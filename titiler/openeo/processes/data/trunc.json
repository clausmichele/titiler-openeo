{
    "id": "trunc",
    "summary": "Truncate fractions",
    "description": "The integer part of the number `x`.\n\nThe no-data value `null` is passed through and therefore gets propagated.",
    "categories": [
        "math > rounding"
    ],
    "parameters": [
        {
            "name": "x",
            "description": "A number to truncate.",
            "schema": {
                "type": [
                    "number",
                    "null"
                ]
            }
        }
    ],
    "returns": {
        "description": "The number truncated.",
        "schema": {
            "type": [
                "integer",
                "null"
            ]
        }
    },
    "examples": [
        {
            "arguments": {
                "x": 0
            },
            "returns": 0
        },
        {
            "arguments": {
                "x": 3.5
            },
            "returns": 4
        },
        {
            "arguments": {
                "x": -0.4
            },
            "returns": 0
        },
        {
            "arguments": {
                "x": -3.5
            },
            "returns": -3
        }
    ],
    "links": [
        {
            "rel": "about",
            "href": "http://mathworld.wolfram.com/CeilingFunction.html",
            "title": "Ceiling explained by Wolfram MathWorld"
        }
    ]
}